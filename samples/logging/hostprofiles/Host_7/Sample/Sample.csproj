<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net461</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="log4net" Version="2.0.8" />
    <PackageReference Include="NServiceBus" Version="6.*" />
    <PackageReference Include="NServiceBus.Host" Version="7.*" />
    <PackageReference Include="NServiceBus.Log4Net" Version="2.0.*" />
  </ItemGroup>
  <!--startcode specific-profile -->
  <PropertyGroup>
    <StartAction>Program</StartAction>
    <StartProgram>$(ProjectDir)$(OutputPath)NServiceBus.Host.exe</StartProgram>
    <StartArguments>NServiceBus.Lite</StartArguments>
  </PropertyGroup>
  <!--endcode-->
</Project>
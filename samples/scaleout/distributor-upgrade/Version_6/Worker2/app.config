<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="MasterNodeConfig" 
             type="NServiceBus.Config.MasterNodeConfig, NServiceBus.Core" />
    <section name="UnicastBusConfig" 
             type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core" />
  </configSections>
  <appSettings>
    <add 
      key="NServiceBus/Distributor/WorkerNameToUseWhileTesting" 
      value="Samples.Scaleout.Worker2" />
  </appSettings>
  <MasterNodeConfig Node="localhost" />
  <UnicastBusConfig 
    DistributorControlAddress="Samples.Scaleout.Master.Distributor.control@localhost" 
    DistributorDataAddress="Samples.Scaleout.Master@localhost"/>
</configuration>
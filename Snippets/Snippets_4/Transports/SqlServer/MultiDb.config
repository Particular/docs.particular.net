<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <!--startcode sqlserver-multidb-connectionstrings 1.2 -->
  <connectionStrings>
    <add name="NServiceBus/Transport" connectionString="Server=DbServerA;Database=MyDefaultDB;"/>
    <add name="NServiceBus/Transport/Billing" connectionString="Server=DbServerB;Database=Billing;"/>
  </connectionStrings>
  <!--endcode-->
  <!--startcode sqlserver-multidb-messagemapping 1.2 -->
  <UnicastBusConfig>
    <MessageEndpointMappings>
      <add Messages="Billing.Contract" Endpoint="billing"/>
      <add Messages="Sales.Contract" Endpoint="sales"/>
    </MessageEndpointMappings>
  </UnicastBusConfig>
  <!--endcode-->
</configuration>
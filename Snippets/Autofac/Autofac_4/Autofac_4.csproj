<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net462</TargetFramework>
    <AssemblyName>Autofac_4</AssemblyName>
    <PackageId>Autofac_4</PackageId>
    <RuntimeIdentifiers>win</RuntimeIdentifiers>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Autofac" Version="3.*" />
    <PackageReference Include="NServiceBus" Version="4.*" />
    <PackageReference Include="NServiceBus.Autofac" Version="4.*" />
    <PackageReference Include="NServiceBus.Interfaces" Version="4.*" />
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'net462' ">
    <Reference Include="System" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>

</Project>

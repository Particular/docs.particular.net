<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <!--startcode sqlserver-multidb-current-endpoint-schema-config 1.2.3 -->
  <connectionStrings>
    <add name="NServiceBus/Transport" connectionString="Data Source=INSTANCE_NAME; Initial Catalog=some_database; Integrated Security=True; Queue Schema=nsb"/>
  </connectionStrings>
  <!--endcode-->
  <!--startcode sqlserver-multidb-connectionstrings 2 -->
  <connectionStrings>
    <add name="NServiceBus/Transport" connectionString="Server=DbServerA;Database=MyDefaultDB;"/>
    <add name="NServiceBus/Transport/Billing" connectionString="Server=DbServerB;Database=Billing;"/>
  </connectionStrings>
  <!--endcode-->
  <!--startcode sqlserver-multidb-messagemapping 2 -->
  <UnicastBusConfig>
    <MessageEndpointMappings>
      <add Assembly="Billing.Contract" Endpoint="billing"/>
      <add Assembly="Sales.Contract" Endpoint="sales"/>
    </MessageEndpointMappings>
  </UnicastBusConfig>
  <!--endcode-->
</configuration>
